@model IEnumerable<mvc3839_21622.Models.tblUser>
@{
    ViewBag.Title = "ShowUser";
}

<h2>ShowUser</h2>
<table border="1" class="table bg-success">
    <tr>
        <th>User Id</th>
        <th>User Name</th>
        <th>User Age</th>
        <th>User Image</th>
        <th></th>
        <th></th>
    </tr>
    @foreach(var a in Model)
            {
                <tr>
                    <td>@a.userid</td>
                    <td>@a.name</td>
                    <td>@a.age</td>
                    <td><img src="@Url.Content(a.img)" style="width:80px;height:60px" /></td>
                    <td>@Html.ActionLink("Edit","AddUser",new {id=a.userid })</td>
                    <td>@Html.ActionLink("Delete","Del",new {id=a.userid ,img=a.img})</td>
                </tr>
            }
</table>
